{

    "manifest_version": 2,
    "name": "LibDetector",
    "version": "1.0",
  
    "description": "Detect JavaScript libraries used on the current web page.",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["libdetector.js"],
        "run_at": "document_end"
      }
    ]
  
  }
  